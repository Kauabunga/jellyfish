{
  "name": "jellyfish",
  "version": "1.0.0",
  "description": "Cucumber test automation scaffold",
  "repository": "https://github.com/solnet-aquarium/jellyfish",
  "main": "index.js",
  "scripts": {
    "start": "npm run show-report",
    "test": "npm run cucumber",
    "setup": "npm run webdriver-manager:update && npm run typings",
    "typings": "typings install",
    "cucumber": "npm-run-all -c -s cucumber:autocomplete cucumber:protractor cucumber:report",
    "cucumber:protractor": "rimraf ./reports/cucumber && protractor ./cucumber.conf.js",
    "cucumber:report": "node ./cucumber.report.js",
    "cucumber:autocomplete": "npm run cucumber:clean && node ./cucumber.generate.js ./features/steps_definitions",
    "cucumber:clean": "rimraf ./node_modules/cucumber-html-report/features",
    "show-report": "http-server ./reports/cucumber -o -p 4444",
    "heroku": "http-server ./reports/cucumber -p $PORT",
    "webdriver-manager": "webdriver-manager",
    "webdriver-manager:update": "webdriver-manager update",
    "webdriver-manager:update:ie": "webdriver-manager update --ie",
    "webdriver-manager:start": "webdriver-manager start",
    "create-document": "dogfish sprint-board --included-sections 'Feature,Acceptance Criteria' --output-file ./reports/features.docx --features-path ./features"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axe-core": "^2.0.5",
    "axe-webdriverjs": "^0.4.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chalk": "^1.1.3",
    "cucumber": "^1.3.1",
    "cucumber-html-report": "^0.4.4",
    "cucumber-tsflow": "^1.1.2",
    "dogfish": "^1.4.2",
    "fs-extra": "^1.0.0",
    "glob": "^7.1.1",
    "http-server": "^0.9.0",
    "npm-run-all": "^3.1.1",
    "protractor": "^4.0.11",
    "protractor-cucumber-framework": "^0.6.0",
    "rimraf": "^2.5.4",
    "ts-node": "^1.7.0",
    "typescript": "^2.0.10",
    "typings": "^2.0.0"
  }
}
